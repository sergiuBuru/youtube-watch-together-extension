{
  "name": "Youtube Watch Together Extension",
  "description": "complete later..",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*","*://www.youtube.com/*"],
      "run_at": "document_end",
      "js": ["script.js"]
    }
  ],
  "background": {
    "service_worker": "sw.js"
  },
  "permissions": ["scripting", "activeTab", "tabs", "storage"],
  "action": {
    "default_popup": "popup.html"
  }
}